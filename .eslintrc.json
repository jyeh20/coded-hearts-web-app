{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "react-app"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
      "react/prop-types": 0,
      "import/prefer-default-export": 0,
      "no-useless-escape": "warn",
      "no-console": "warn",
      "no-useless-catch": 0,
      "react/jsx-props-no-spreading": 0,
      "import/no-named-as-default-member": 1,
      "react/jsx-filename-extension": [
        "warn",
        {
          "extensions": [".js", ".jsx"]
        }
      ]
    }
}
